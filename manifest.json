{
  "manifest_version": 2,

  "name": "Connectome",
  "description": "Send invites and messages from anywhere.",
  "version": "1.0",

  "permissions": [
    "tabs", "<all_urls>"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },
  
  "background": {
    "scripts": ["background.js"]
  }
  
}